<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.elling.rent.dao.mapper.RentContractMapper">
  <resultMap id="BaseResultMap" type="com.elling.rent.model.RentContract">
    <id column="ID" jdbcType="BIGINT" property="id" />
    <result column="CONTRACT_CODE" jdbcType="VARCHAR" property="contractCode" />
    <result column="CONTRACT_NAME" jdbcType="VARCHAR" property="contractName" />
    <result column="HOUSE_CODE" jdbcType="VARCHAR" property="houseCode" />
    <result column="PERSON_CODE" jdbcType="VARCHAR" property="personCode" />
    <result column="SIGN_TIME" jdbcType="VARCHAR" property="signTime" />
    <result column="START_TIME" jdbcType="VARCHAR" property="startTime" />
    <result column="END_TIME" jdbcType="VARCHAR" property="endTime" />
    <result column="RENTER" jdbcType="VARCHAR" property="renter" />
    <result column="DEPOSIT" jdbcType="VARCHAR" property="deposit" />
    <result column="WATER_CLOSE_TYPE" jdbcType="VARCHAR" property="waterCloseType" />
    <result column="PAY_TYPE" jdbcType="VARCHAR" property="payType" />
    <result column="STATUS" jdbcType="VARCHAR" property="status" />
    <result column="ORDER_NUM" jdbcType="BIGINT" property="orderNum" />
    <result column="REMARK" jdbcType="VARCHAR" property="remark" />
    <result column="CREATE_TIME" jdbcType="VARCHAR" property="createTime" />
    <result column="UPDATE_TIME" jdbcType="VARCHAR" property="updateTime" />
  </resultMap>
  <resultMap id="customerMap" type="java.util.HashMap">
    <id column="ID" jdbcType="BIGINT" property="id" />
    <result column="CONTRACT_CODE" jdbcType="VARCHAR" property="contractCode" />
    <result column="CONTRACT_NAME" jdbcType="VARCHAR" property="contractName" />
    <result column="HOUSE_CODE" jdbcType="VARCHAR" property="houseCode" />
    <result column="PERSON_CODE" jdbcType="VARCHAR" property="personCode" />
    <result column="SIGN_TIME" jdbcType="VARCHAR" property="signTime" />
    <result column="START_TIME" jdbcType="VARCHAR" property="startTime" />
    <result column="END_TIME" jdbcType="VARCHAR" property="endTime" />
    <result column="RENTER" jdbcType="VARCHAR" property="renter" />
    <result column="DEPOSIT" jdbcType="VARCHAR" property="deposit" />
    <result column="WATER_CLOSE_TYPE" jdbcType="VARCHAR" property="waterCloseType" />
    <result column="PAY_TYPE" jdbcType="VARCHAR" property="payType" />
    <result column="STATUS" jdbcType="VARCHAR" property="status" />
    <result column="ORDER_NUM" jdbcType="BIGINT" property="orderNum" />
    <result column="REMARK" jdbcType="VARCHAR" property="remark" />
    <result column="CREATE_TIME" jdbcType="VARCHAR" property="createTime" />
    <result column="UPDATE_TIME" jdbcType="VARCHAR" property="updateTime" />
  </resultMap>
  <!--根据自定义条件查询-->
  <select id="getByCondition" resultMap="customerMap">
	    SELECT
			<include refid="BaseColumn"/>
		FROM
			RENT_CONTRACT T
		<where>
			<if test="id!=null and id != ''">T.ID = #{id}</if>
			<if test="contractCode!=null and contractCode != ''">T.CONTRACT_CODE = #{contractCode}</if>
			<if test="contractName!=null and contractName != ''">T.CONTRACT_NAME = #{contractName}</if>
			<if test="houseCode!=null and houseCode != ''">T.HOUSE_CODE = #{houseCode}</if>
			<if test="personCode!=null and personCode != ''">T.PERSON_CODE = #{personCode}</if>
			<if test="signTime!=null and signTime != ''">T.SIGN_TIME = #{signTime}</if>
			<if test="startTime!=null and startTime != ''">T.START_TIME = #{startTime}</if>
			<if test="endTime!=null and endTime != ''">T.END_TIME = #{endTime}</if>
			<if test="renter!=null and renter != ''">T.RENTER = #{renter}</if>
			<if test="deposit!=null and deposit != ''">T.DEPOSIT = #{deposit}</if>
			<if test="waterCloseType!=null and waterCloseType != ''">T.WATER_CLOSE_TYPE = #{waterCloseType}</if>
			<if test="payType!=null and payType != ''">T.PAY_TYPE = #{payType}</if>
			<if test="status!=null and status != ''">T.STATUS = #{status}</if>
			<if test="orderNum!=null and orderNum != ''">T.ORDER_NUM = #{orderNum}</if>
			<if test="remark!=null and remark != ''">T.REMARK = #{remark}</if>
			<if test="createTime!=null and createTime != ''">T.CREATE_TIME = #{createTime}</if>
			<if test="updateTime!=null and updateTime != ''">T.UPDATE_TIME = #{updateTime}</if>
		</where>
  </select>
  <sql id="BaseColumn">
  		CONTRACT_CODE,CONTRACT_NAME,HOUSE_CODE,PERSON_CODE,SIGN_TIME,START_TIME,END_TIME,RENTER,DEPOSIT,WATER_CLOSE_TYPE,PAY_TYPE,STATUS,ORDER_NUM,REMARK,CREATE_TIME,UPDATE_TIME
  </sql>
</mapper>